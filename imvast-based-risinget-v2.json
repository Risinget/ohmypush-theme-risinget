{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#4f4f4f",
          "style": "plain",
          "template": "\u256d\u2500",
          "type": "text"
        },
        {
          "foreground": "#EFDCF9",
          "properties": {
            "arch": "\uf303",
            "debian": "\uf306",
            "fedora": "\uf30a",
            "linux": "\uf17c",
            "macos": "\uf179",
            "manjaro": "\uf312",
            "opensuse": "\uf314",
            "ubuntu": "\uf31b",
            "windows": "\uf17a"
          },
          "style": "diamond",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": " {{ .UserName }} on",
          "type": "text"
        },
        {
          "foreground": "#695ee2",
          "style": "plain",
          "template": " 00-00",
          "type": "text"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": " at",
          "type": "text"
        },
        {
          "foreground": "#5da1be",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": "\\",
            "home_icon": " \uedfe",
            "style": "full"
          },
          "style": "powerline",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#695ee2",
          "properties": {
            "time_format": "3:04 PM"
          },
          "style": "plain",
          "template": "\uf017 {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "via",
          "type": "text"
        },
        {
          "foreground": "#dd266c",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "plain",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "python"
        },
        {
          "type": "command",
          "style": "plain",
          "foreground": "#FF2D55",
          "properties": {
            "shell": "powershell",
            "command": "(php artisan --version) -match '(\\d+\\.\\d+\\.\\d+)' | Out-Null; $Matches[1]"
          },
          "template": " \ue73F v{{ .Output }}"
        },
        {
          "foreground": "#7056ff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "plain",
          "template": " \ue608 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "php"
        },
        // {
        //   "type": "command",
        //   "style": "plain",
        //   "foreground": "#ff9136",
        //   "properties": {
        //     "shell": "powershell",
        //     "command": "if (Test-Path 'composer.json') { if (composer --version 2>$null) { (composer --version) -match '(\\d+\\.\\d+\\.\\d+)' | Out-Null; $Matches[1] } }"
        //   },
        //   "template": " \uf487 v{{ .Output }}"
        // },
        // {
        //   "type": "node",
        //   "style": "powerline",
        //   "foreground": "#45bf17",
        //   "template": " \ue718 {{ .Full }} "
        // },
        // {
        //   "type": "npm",
        //   "style": "powerline",
        //   "foreground": "#FE4A49",
        //   "template": "<#F3EFF5></>\ue71e {{ .Full }} "
        // },
        {
          "foreground": "#ffa238",
          "powerline_symbol": "\ue0b0",
          "style": "plain",
          "template": " \ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "java"
        },
        {
          "foreground": "#ff0062",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "white",
          "properties": {
            "always_enabled": true,
            "style": "round"
          },
          "style": "plain",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "foreground": "green",
          "foreground_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " {{ if gt .Code 0 }}\uea76{{else}}\uf42e{{ end }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#4f4f4f",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "foreground": "#4f4f4f",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "â†’ ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}
